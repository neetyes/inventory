<div class="container">
  <h3 class="col-md-offset-4">Company Profile</h3>
  <div class="col-md-4 col-md-offset-4 well">
      <div class="row">
        <div class="col-md-5">
          <p><b>Name:</b></p>
          <p><b>VDC/Municipality:</b></p>
          <p><b>Ward no.:</b></p>
          <p><b>Phone:</b></p>
          <p><b>VAT pan no.:</b></p>
          <p><b>Logo url:</b></p>
          <p><b>District:</b></p>
          <p><b>Zone:</b></p>
          <p><b>Fiscal year id:</b></p>
        </div>
        <div class="col-md-7">
          <p><%= @company_profile.name %></p>
          <p><%= @company_profile.vdc_mun %></p>
          <p><%= @company_profile.ward_no %></p>
          <p><%= @company_profile.phone %></p>
          <p><%= @company_profile.vat_pan_no %></p>
          <p><%= @company_profile.logo_url %></p>
          <p><%= @company_profile.district %></p>
          <p><%= @company_profile.zone %></p>
          <p><%= @company_profile.fiscal_year_id %></p>
        </div>
      </div>
    <div class="col-md-6 col-md-offset-3">
      <a class="btn btn-success btn-block" href="<%= edit_company_profile_path %>">
        <i class="fa fa-plus" aria-hidden="true">
          Edit Profile </i>
      </a>
    </div>
    </div>
</div>