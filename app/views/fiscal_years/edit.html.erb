<body>
<%= form_for (@fiscal_year), :html => {:class => "form-horizontal"} do |f| %>
    <div class="col-md-5 col-md-offset-3 well">
      <div class="form-group">
        <%= f.label :name, :class => "col-md-3" %>
        <div class="col-md-9"><%= f.text_field :name, :class => "form-control", :placeholder => "name" %> </div>
      </div>
      <div class="form-group">
        <%= f.label :end_date, :class => "col-md-3" %>
        <div class="col-md-9 date">
          <%= f.text_field :end_date, :class => "form-control ", :id => "start_date", :placeholder => "YYYY/MM/DD" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :end_date, :class => "col-md-3" %>
        <div class="col-md-9 date">
          <%= f.text_field :end_date, :class => "form-control ", :id => "end_date", :placeholder => "YYYY/MM/DD" %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-md-offset-3 col-md-9">
          <%= button_tag(type: "submit", class: "btn btn-success") do %>
              Update
          <% end %>
        </div>
      </div>
    </div>
<% end %>
<script>
    $('#start_date').datepicker({
        format: "yyyy/mm/dd",
    }).on('changeDate', function (e) {
        $(this).datepicker('hide');
    });
</script>
<script>
    $('#end_date').datepicker({
        format: "yyyy/mm/dd",
    }).on('changeDate', function (e) {
        $(this).datepicker('hide');
    });
</script>

</body>